module Views

templates

  Section.Views = <
    views
    
      <View*>
  >
  
  View.OutlineView = <
    outline view: <StrategoCall>
      <OutlineOption*; separator = "\n">
  >
  
  OutlineOption.ExpandToLevel = <expand to level: <Int>>
  
  View.PropertiesView = <
    properties view: <StrategoCall>
      <PropertiesOption*; separator = "\n">
  >  
  
  PropertiesOption.Source = <(source)>
  
template options

  tokenize : ":()"
